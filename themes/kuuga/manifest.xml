<!--
    unique identifier
-->

<lotc>
    <theme_name>Kuuga</theme_name>
    <version>1.0</version>
    <author>Rania Amina</author>
    <description>Kuuga is LO-TC theme with dark accent. This theme inspired by Kuuga Series, Japanese super hero that usually called by Kamen Rider or Masked Rider.</description>
    <assets id="personas">
        <persona_list>personas/personas_list.txt</persona_list>
        <footer_img>personas/kuuga/footer.png</footer_img>
        <header_img>personas/kuuga/header.png</header_img>
        <preview>personas/kuuga/preview.png</preview>
    </assets>
    <assets id="program">
        <intro>program/intro.png</intro>
        <soffice>program/sofficerc</soffice>
    </assets>
    <assets id="screnshots">
        <img id="screenshot-1">screenshots/screenshot.png</img>
        <img id="screenshot-2">screenshots/screenshot-2.png</img>
    </assets>
</lotc>
